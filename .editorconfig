# http://editorconfig.org

root = true

[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2
charset = utf8
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.markdown]
trim_trailing_whitespace = false

[*.mdown]
trim_trailing_whitespace = false
